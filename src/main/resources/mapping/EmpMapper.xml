<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.accp.dao.IEmpDao">

	<select id="queryAll" resultType="com.accp.pojo.Emp">
		select * from tbl_emp where did=#{id}
	</select>

	<delete id="deleteEmp">
		delete from tbl_emp where did=#{id}
	</delete>
</mapper>    